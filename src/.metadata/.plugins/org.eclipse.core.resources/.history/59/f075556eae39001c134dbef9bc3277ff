package proyecto3;

public class Posicion {
	
	private int celda;
	private int colum;
	private boolean habitable;
	private boolean hayPickUp;
	
	public Posicion(int celda, int colum, boolean habitable, boolean hayPickUp) {
		this.setCelda(celda);
		this.setColum(colum);
		this.setHabitable(habitable);
		this.setHayPickUp(hayPickUp);
	}

	public int getCelda() {
		return celda;
	}

	public void setCelda(int celda) {
		this.celda = celda;
	}

	public int getColum() {
		return colum;
	}

	public void setColum(int colum) {
		this.colum = colum;
	}

	public boolean isHabitable() {
		return habitable;
	}

	public void setHabitable(boolean habitable) {
		this.habitable = habitable;
	}

	public boolean isHayPickUp() {
		return hayPickUp;
	}

	public void setHayPickUp(boolean hayPickUp) {
		this.hayPickUp = hayPickUp;
	}
	
}
