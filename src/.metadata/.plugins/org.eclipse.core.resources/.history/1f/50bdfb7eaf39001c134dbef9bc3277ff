package proyecto3;

public class Posicion {
	
	private int celda;
	private int colum;
	private boolean habitable;
	private PickUp pickUp;
	
	public Posicion(int celda, int colum, boolean habitable, PickUp pu) {
		this.setCelda(celda);
		this.setColum(colum);
		this.setHabitable(habitable);
		this.setHayPickUp(pickUp);
	}

	public int getCelda() {
		return celda;
	}

	public void setCelda(int celda) {
		this.celda = celda;
	}

	public int getColum() {
		return colum;
	}

	public void setColum(int colum) {
		this.colum = colum;
	}

	public boolean isHabitable() {
		return habitable;
	}

	public void setHabitable(boolean habitable) {
		this.habitable = habitable;
	}

	public boolean HayPickUp() {
		return pickUp!=null;
	}

	public void setHayPickUp(PickUp pu) {
		this.pickUp = pu;
	}
	
	public PickUp getPickUp() {
		return pickUp;
	}
	
}
